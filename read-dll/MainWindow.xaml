        <ListBox x:Name="listbox1" HorizontalAlignment="Left" Height="355" Margin="35,54,0,0" VerticalAlignment="Top" Width="203"  SelectionChanged="Listbox1_SelectionChanged"/>
        <GroupBox x:Name="groupbox" Header="class" HorizontalAlignment="Left" Height="355" Margin="254,54,0,0" VerticalAlignment="Top" Width="401"/>

    </Grid>
