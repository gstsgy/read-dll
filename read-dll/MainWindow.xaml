        <TextBox x:Name="text1" HorizontalAlignment="Left" Height="24" Margin="35,25,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="502" FontSize="14" TextDecorations="{x:Null}" IsReadOnly="True"/>
